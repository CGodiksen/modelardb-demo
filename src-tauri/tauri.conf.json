{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "modelardb-demo",
  "version": "0.1.0",
  "identifier": "com.modelardb-demo.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ModelarDB",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "data/nodes.json": "resources/nodes.json",
      "data/wind_cleaned.parquet": "resources/wind_cleaned.parquet"
    }
  }
}
